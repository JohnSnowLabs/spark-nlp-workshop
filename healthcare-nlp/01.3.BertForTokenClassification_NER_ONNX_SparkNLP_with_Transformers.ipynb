        "print(f\"✓ Saved labels.txt ({len(labels_sorted)} labels)\")\n",
        "\n",
        "# 6. Copy vocab.txt to assets\n",
        "vocab_src = os.path.join(OUTPUT_DIR, \"vocab.txt\")\n",
        "if os.path.exists(vocab_src):\n",
