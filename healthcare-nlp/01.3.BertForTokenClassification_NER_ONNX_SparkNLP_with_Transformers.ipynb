        "os.makedirs(assets_path, exist_ok=True)\n",
        "\n",
        "# 2. Copy ONNX model to ROOT as model.onnx\n",
        "onnx_dest = os.path.join(SPARK_NLP_PATH, \"model.onnx\")\n",
