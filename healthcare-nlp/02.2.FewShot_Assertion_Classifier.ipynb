    {
      "cell_type": "markdown",
      "metadata": {
        "id": "JjfUo0aTKFEL"
      },
      "source": [
        "# Train a custom Few-Shot Assertion Model"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 12,
      "metadata": {
        "id": "QPFTJxtlIKQT"
      },
      "outputs": [],
      "source": [
        "!wget -q https://raw.githubusercontent.com/JohnSnowLabs/spark-nlp-workshop/master/tutorials/Certification_Trainings/Healthcare/data/i2b2_assertion_sample_short.csv"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 13,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "TQn7bJZtL9uX",
        "outputId": "a836628c-2b2a-4372-c6a4-0635e23c1a33"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "+-------------------------------------------------+-------------------+-------+-----+---+\n",
            "|                                             text|             target|  label|start|end|\n",
            "+-------------------------------------------------+-------------------+-------+-----+---+\n",
            "|She has no history of liver disease , hepatitis .|      liver disease| absent|    5|  6|\n",
            "|                         1. Undesired fertility .|undesired fertility|present|    1|  2|\n",
            "|                            3) STATUS POST FALL .|               fall|present|    3|  3|\n",
            "+-------------------------------------------------+-------------------+-------+-----+---+\n",
            "only showing top 3 rows\n",
            "\n"
          ]
        }
      ],
      "source": [
        "import pandas as pd\n",
        "\n",
        "assertion_df = spark.read.option(\"header\", True).option(\"inferSchema\", \"True\").csv(\"i2b2_assertion_sample_short.csv\")\n",
        "\n",
        "assertion_df.show(3, truncate=100)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 14,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "B6IuDZ9Qw7Cu",
        "outputId": "781ab052-dc6c-423d-a19d-1ec5b71c9bb5"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Training Dataset Count: 721\n",
            "Test Dataset Count: 170\n"
          ]
        }
      ],
      "source": [
        "(training_data, test_data) = assertion_df.randomSplit([0.8, 0.2], seed = 100)\n",
        "print(\"Training Dataset Count: \" + str(training_data.count()))\n",
        "print(\"Test Dataset Count: \" + str(test_data.count()))"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "training_data.groupBy('label').count().orderBy('count', ascending=False).show(truncate=False)\n",
        "\n",
        "test_data.groupBy('label').count().orderBy('count', ascending=False).show(truncate=False)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "RMBBJT4QFiU1",
        "outputId": "8d3cd02c-a74a-4c54-ddea-8cd30be87bb0"
      },
      "execution_count": 15,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "+-------+-----+\n",
            "|label  |count|\n",
            "+-------+-----+\n",
            "|present|546  |\n",
            "|absent |175  |\n",
            "+-------+-----+\n",
            "\n",
            "+-------+-----+\n",
            "|label  |count|\n",
            "+-------+-----+\n",
            "|present|117  |\n",
            "|absent |53   |\n",
            "+-------+-----+\n",
            "\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "document = nlp.DocumentAssembler()\\\n",
        "    .setInputCol(\"text\")\\\n",
        "    .setOutputCol(\"document\")\n",
        "\n",
        "token = nlp.Tokenizer()\\\n",
        "    .setInputCols([\"document\"])\\\n",
        "    .setOutputCol(\"token\")\n",
        "\n",
        "chunk2doc = medical.Doc2ChunkInternal()\\\n",
        "    .setInputCols([\"document\",\"token\"])\\\n",
        "    .setOutputCol(\"ner_chunk\")\\\n",
        "    .setChunkCol(\"target\")\\\n",
        "    .setStartCol(\"start\")\\\n",
        "    .setStartColByTokenIndex(True)\\\n",
        "    .setFailOnMissing(False)\\\n",
        "    .setLowerCase(True)\n",
        "\n",
        "few_shot_assertion_sentence_converter = medical.FewShotAssertionSentenceConverter()\\\n",
        "    .setInputCols([\"document\", \"token\",\"ner_chunk\"])\\\n",
        "    .setOutputCol(\"assertion_sentence\")\n",
        "\n",
        "e5_embeddings = nlp.E5Embeddings.pretrained(\"e5_base_v2_embeddings_medical_assertion_base\", \"en\", \"clinical/models\")\\\n",
        "    .setInputCols([\"assertion_sentence\"])\\\n",
        "    .setOutputCol(\"assertion_embedding\")\n",
        "\n",
        "embeddings_pipeline = nlp.Pipeline(\n",
        "    stages = [\n",
        "        document,\n",
        "        token,\n",
        "        chunk2doc,\n",
        "        few_shot_assertion_sentence_converter,\n",
        "        e5_embeddings\n",
        "])"
      ],
      "metadata": {
        "id": "ar82Klv5OQ4C",
        "outputId": "c820ebaf-b7e9-4604-96f7-7287e0027e4d",
        "colab": {
          "base_uri": "https://localhost:8080/"
        }
      },
      "execution_count": 16,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "e5_base_v2_embeddings_medical_assertion_base download started this may take some time.\n",
            "Approximate size to download 374.8 MB\n",
            "[OK!]\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 17,
      "metadata": {
        "id": "-7DScL2-uIOm"
      },
      "outputs": [],
      "source": [
        "assertion_test_data = embeddings_pipeline.fit(test_data).transform(test_data)\n",
        "#assertion_test_data.write.mode('overwrite').parquet('i2b2_assertion_sample_test_data.parquet')\n",
        "\n",
        "assertion_train_data = embeddings_pipeline.fit(training_data).transform(training_data)\n",
        "#assertion_train_data.write.mode('overwrite').parquet('i2b2_assertion_sample_train_data.parquet')"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "uTishXbut1MS"
      },
      "source": [
        "## Graph setup"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "1hTawAHemzGn"
      },
      "outputs": [],
      "source": [
        "!pip install -q tensorflow==2.12.0\n",
        "!pip install -q tensorflow-addons"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "0ShZT8BBo4FY"
      },
      "source": [
        "We will use TFGraphBuilder annotator which can be used to create graphs in the model training pipeline.\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "from sparknlp_jsl.annotator import TFGraphBuilder\n",
        "\n",
        "graph_folder = \"./tf_graphs\"\n",
        "graph_name = \"assertion_graph.pb\"\n",
        "\n",
        "assertion_graph_builder = medical.TFGraphBuilder()\\\n",
        "    .setModelName(\"fewshot_assertion\")\\\n",
        "    .setInputCols([\"assertion_embedding\"]) \\\n",
        "    .setLabelColumn(\"label\")\\\n",
        "    .setGraphFolder(graph_folder)\\\n",
        "    .setGraphFile(graph_name)\\\n",
        "    .setHiddenUnitsNumber(100)\n",
        "\n",
        "fewshot_assertion_approach = medical.FewShotAssertionClassifierApproach()\\\n",
        "    .setInputCols(\"assertion_embedding\")\\\n",
        "    .setOutputCol(\"assertion\")\\\n",
        "    .setLabelCol(\"label\")\\\n",
        "    .setBatchSize(32)\\\n",
        "    .setDropout(0.1)\\\n",
        "    .setLearningRate(0.001)\\\n",
        "    .setEpochsNumber(40)\\\n",
        "    .setValidationSplit(0.2)\\\n",
        "    .setModelFile(f\"{graph_folder}/{graph_name}\")\n",
        "\n",
        "clinical_assertion_pipeline = nlp.Pipeline(\n",
        "    stages = [\n",
        "        assertion_graph_builder,\n",
        "        fewshot_assertion_approach\n",
        "])"
      ],
      "metadata": {
        "id": "9gURcv-NCywm"
      },
      "execution_count": 20,
      "outputs": []
    },
    {
      "cell_type": "code",
      "execution_count": 21,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "Ei54XasnMU0U",
        "outputId": "a1d4fce6-f685-4ef0-f098-bd3c708fd3d4"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "TF Graph Builder configuration:\n",
            "Model name: fewshot_assertion\n",
            "Graph folder: ./tf_graphs\n",
            "Graph file name: assertion_graph.pb\n",
            "Build params: {'input_dim': 768, 'output_dim': 2}\n",
            "fewshot_assertion graph exported to ./tf_graphs/assertion_graph.pb\n",
            "CPU times: user 3.14 s, sys: 308 ms, total: 3.45 s\n",
            "Wall time: 32.1 s\n"
          ]
        }
      ],
      "source": [
        "%%time\n",
        "\n",
        "assertion_model = clinical_assertion_pipeline.fit(assertion_train_data)"
      ]
